generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Scan {
  id         String   @id @default(cuid())
  url        String
  riskScore  Int
  riskLevel  String
  reasons    Json
  createdAt  DateTime @default(now())
}

model Report {
  id        String   @id @default(cuid())
  url       String
  reason    String
  createdAt DateTime @default(now())
}
